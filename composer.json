{
    "name": "jsonbaby/websocket-connection",
    "description": "",
    "keywords": [
        "jsonbaby",
        "websocket-connection"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Olexandr Dolbin",
            "email": "olexandr.dolbin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.0",
        "jsonbaby/event-bridge": "dev-main",
        "textalk/websocket": "^1.5"
    },
    "require-dev": {
        "orchestra/testbench": "^7.1",
        "phpunit/phpunit": "^9.5.8",
        "vimeo/psalm": "5.x-dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "JsonBaby\\WebsocketConnection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonBaby\\WebsocketConnection\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JsonBaby\\WebsocketConnection\\WebsocketConnectionServiceProvider"
            ]
        }
    }
}
